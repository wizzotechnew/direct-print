# =========================
# ANDROPRINT CORE
# =========================
PORT=3000
ENABLE_AUTH=true

# =========================
# FILE PATHS
# =========================
CONFIG_DIR=config
PRINTER_FILE=config/printer.json
CLIENT_FILE=config/clients.json
AUTH_FILE=config/auth.json
SERVER_ID_FILE=config/server.id

# =========================
# CLOUDFLARE TUNNEL
# =========================
CLOUDFLARE=true
CLOUDFLARE_PORT=3000
CLOUDFLARE_LOG=logs/cloudflared.log

# =========================
# JAVA PRINT ENGINE (OPTIONAL)
# =========================
JAVA_PRINT=false
JAVA_PRINT_URL=http://localhost:8090

# =========================
# WEBHOOK PRINTING (OPTIONAL)
# =========================
WEBHOOK_ENABLED=false
WEBHOOK_SECRET=super_long_random_key_here

# =========================
# DATABASE (FOR JASPER / JAVA)
# =========================
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=your_database_name
DB_USER=your_db_user
DB_PASS=your_db_password
