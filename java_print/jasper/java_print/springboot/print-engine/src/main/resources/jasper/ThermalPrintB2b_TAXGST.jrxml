	<queryString>
		<![CDATA[SELECT
     SalesBill.`SiNo` AS SalesBill_SiNo,
     SalesBill.`Item` AS SalesBill_Item,
     SalesBill.`Qty` AS SalesBill_Qty,
     SalesBill.`Rate` AS SalesBill_Rate,
     MasterDetails.`BillDate` AS MasterDetails_BillDate,
     MasterDetails.`BillPartyName` AS MasterDetails_BillPartyName,
     MasterDetails.`BillNetTotalField` AS MasterDetails_BillNetTotalField,
     MasterDetails.`BillTime` AS MasterDetails_BillTime,
     SalesBill.`NetV` AS SalesBill_NetV,
     SalesBill.`ItemC` AS SalesBill_ItemC,
     SalesBill.`ItWiseDisAmt` AS SalesBill_ItWiseDisAmt,
     SalesBill.`Dis` AS SalesBill_Dis,
     MasterDetails.`del_date` AS MasterDetails_del_date,
     SalesBill.`Total` AS SalesBill_Total,
     SalesBill.`TaxP` AS SalesBill_TaxP,
     SalesBill.`TaxA` AS SalesBill_TaxA,
     MasterDetails.`DCBillNo` AS MasterDetails_DCBillNo,
     MasterDetails.`TItTaxAmt` AS MasterDetails_TItTaxAmt
FROM
     `SalesBill` SalesBill,
     `MasterDetails` MasterDetails
WHERE
     SalesBill.BillNo = $P{NBillNo}
 AND MasterDetails.BillNo = $P{MasterDet}
 AND SalesBill.BillTyp = '8'
ORDER BY
     SalesBill.SiNo ASC]]>
	</queryString>
